<!DOCTYPE suite SYSTEM "testng-1.0.dtd">

<suite name="Nopcommerce" verbose="1">

    <listeners>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test name="Run on Firefox" preserve-order="false">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.nopcommerce.cookie.Common_Register"/>
            <class name="com.nopcommerce.cookie.Product"/>
        </classes>
    </test>



</suite>